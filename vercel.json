{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOWALL" },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://sites.google.com https://*.google.com https://*.googleusercontent.com;"
        }
      ]
    }
  ]
}
